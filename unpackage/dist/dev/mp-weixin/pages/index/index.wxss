
.main-classify{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	direction: center;
}
.classify-img{
	width: 200rpx;
	height: 200rpx;
	padding: 20rpx;
}
.simepleTips{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	font-size: 36rpx;
	color:#39a13e;
}
.swiper-img{
	width: 750rpx;
	height: 300rpx;
}
.swiper-flex{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.tip-text{
	text-align: center;
	margin-top: 5rpx;
	font-size: 24rpx;
}
.input-view{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border: 1rpx solid #00f375;
	border-radius: 50rpx;
	box-shadow: 10rpx 10rpx 20rpx #FFFFFF;
	margin: 10rpx 20rpx 20rpx 20rpx 20rpx;
}
.search-img{
	width: 80rpx;
	height: 80rpx;
}
.input-view-item{
	height: 80rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 8rpx;
}
.search-text{
	height: 80rpx;
	text-align: center;
	width: 350rpx;
}
.font-search{
	height: 80rpx;
	padding: 10rpx;
	color: #4CD964;
	font-weight: bold;
	margin-top: 30rpx;
}

